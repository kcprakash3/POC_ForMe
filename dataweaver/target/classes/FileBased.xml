    </flow>
    <flow name="FileBasedFlowtoObject">
        <byte-array-to-object-transformer doc:name="Byte Array to Object"/>
        <logger level="INFO" doc:name="Logger"/>
    </flow>
    <flow name="FileBasedFlowtoMIME">
        <file:inbound-endpoint path="E:\SSP_Dev\workspaces\ForMe\dataweaver\src\main\resources\Input" connector-ref="File" responseTimeout="10000" mimeType="text/csv" metadata:id="22e85e45-2211-48eb-90f3-12cfe7e8bd42" doc:name="File"/>
        <email:bytes-to-mime-transformer mimeType="text/csv" doc:name="Byte Array to MIME"/>
        <logger level="INFO" doc:name="Logger"/>
    </flow>
</mule>
